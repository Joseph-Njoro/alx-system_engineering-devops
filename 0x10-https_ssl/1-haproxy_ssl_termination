global
        #dhparam
        tune.ssl.default-dh-param 2048

defaults
  mode http
  timeout client 15s
  timeout connect 10s
  timeout server 15s
  timeout http-request 10s

frontend clickviral-tech-frontend
    bind *:80
    bind *:443 ssl crt /etc/letsencrypt/live/www.ramoz.tech/ramoz.pem

    default_backend clickviral-tech-backend

backend clickviral-tech-backend
    balance roundrobin
    server 499183-web-01 54.172.132.165:80 check
    server 499183-web-02 34.229.67.73:80 check
